defmodule HedwigTest do
	test "Connects to our server" do
		client = %{
			jid: "yellow@localhost",
			password: "thepw",
			nickname: "yellow",
			rooms: [
				"yellow@localhost"
			],
			config: %{
				require_tls?: false,
				use_compression?: false,
				use_stream_management?: true,
				transport: :tcp
			},
			handlers: [{XmppTests.Handlers.Dummy, %{}}]
			   }
		{:ok, _hedwig_pid} = Hedwig.Client.start_link(client)
	end
	
end
